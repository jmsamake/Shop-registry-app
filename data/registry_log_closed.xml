<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ir_cron_set_status_to_closed" model="ir.cron">
            <field name="name">Registry Log Closed</field>
            <field name="model_id" ref="model_registry_app_registry_app"/>
            <field name="state">code</field>
            <field name="code">model._cron_set_status_to_closed()</field>
            <field name="interval_number">1</field>
            <field name="nextcall" eval="(DateTime.now().replace(hour=18, minute=30,) + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')" />
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>
    </data>
</odoo>