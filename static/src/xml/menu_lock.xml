<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <!--security pin modal dialog box template-->
    <div t-name="menuLockPopup" class="d-flex justify-content-center align-items-center reg_app_login">
        <div class="row">
                <div class="row">
                   <form>
                       <div class="form-group">
                          <div style="position: relative; left: 50%; transform: translateX(-50%)">
<!--                              <span class="text-white">User ID: <t t-esc="user_id"/></span>-->
<!--                              <input type="text" class="form-control" id="regUserName" t-att-value="user_id" readonly="True"/>-->
                          </div>
                      </div>
                      <div class="form-group">
                          <div style="position: relative; left: 50%; transform: translateX(-50%)">
                              <input type="password" class="form-control reg_app_password" id="password" placeholder="Enter Password"/>
                              <a type="button" class="fa fa-eye toggle_eye " onclick="myFunction()"
                                 style="position: absolute;top: 0.5rem;right: 0.75rem;"/>
                          </div>
                      </div>
                       <div class="wrong_password_alert" style="display:none;" id="wrong_password_alert">
                           <div id="wrap">
                                <div class="container">
                                    <div class="col-md-12">
                                        <div class="alert alert-danger">
                                            Wrong Password !!!!
                                            Please enter correct password.
                                        </div>
                                    </div>
                                </div>
                            </div>
                       </div>
                  </form>
               </div>
        </div>
    </div>
</templates>
